{
  "name": "@nuxtjs/fontaine",
  "version": "0.4.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/fontaine.git"
  },
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "fonts",
    "cls",
    "web-vitals",
    "performance"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./transform": {
      "types": "./dist/transform.d.ts",
      "import": "./dist/transform.mjs",
      "require": "./dist/transform.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "pnpm nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "docs:dev": "nuxi dev docs",
    "docs:build": "nuxi generate docs",
    "lint": "pnpm lint:all:eslint",
    "lint:all:eslint": "pnpm lint:eslint .",
    "lint:eslint": "eslint --fix",
    "prepack": "pnpm build",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && npm publish",
    "test": "vitest run --coverage",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.159",
    "fontaine": "^0.5.0",
    "magic-string": "^0.30.12",
    "pathe": "^1.1.2",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "0.7.1",
    "@nuxt/module-builder": "0.8.4",
    "@nuxt/schema": "3.14.159",
    "@nuxt/test-utils": "3.14.4",
    "@types/node": "22.9.0",
    "@vitest/coverage-v8": "2.1.5",
    "bumpp": "9.8.1",
    "eslint": "9.14.0",
    "lint-staged": "15.2.10",
    "nuxt": "3.14.159",
    "rollup": "4.27.3",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.6.3",
    "unbuild": "2.0.0",
    "vitest": "2.1.5",
    "vue": "3.5.12"
  },
  "lint-staged": {
    "*.{md,js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "resolutions": {
    "@nuxt/kit": "3.14.159",
    "@nuxtjs/fontaine": "link:."
  },
  "packageManager": "pnpm@9.13.2"
}
