{
  "name": "@nuxtjs/fontaine",
  "version": "0.4.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/fontaine.git"
  },
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "fonts",
    "cls",
    "web-vitals",
    "performance"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "type": "module",
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./transform": {
      "types": "./dist/transform.d.mts",
      "import": "./dist/transform.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "pnpm nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "docs:dev": "nuxi dev docs",
    "docs:build": "nuxi generate docs",
    "lint": "pnpm lint:all:eslint",
    "lint:all:eslint": "pnpm lint:eslint .",
    "lint:eslint": "eslint --fix",
    "prepack": "pnpm build",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && npm publish",
    "test": "vitest run --coverage",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.159",
    "fontaine": "^0.5.0",
    "magic-string": "^0.30.12",
    "pathe": "^2.0.0",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "0.7.4",
    "@nuxt/module-builder": "1.0.0-alpha.1",
    "@nuxt/schema": "3.15.0",
    "@nuxt/test-utils": "3.15.1",
    "@types/node": "22.10.2",
    "@vitest/coverage-v8": "2.1.8",
    "bumpp": "9.9.2",
    "eslint": "9.17.0",
    "lint-staged": "15.2.11",
    "nuxt": "3.15.0",
    "rollup": "4.29.1",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.7.2",
    "unbuild": "3.0.1",
    "vitest": "2.1.8",
    "vue": "3.5.13"
  },
  "lint-staged": {
    "*.{md,js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "resolutions": {
    "@nuxt/kit": "3.15.0",
    "@nuxtjs/fontaine": "link:."
  },
  "packageManager": "pnpm@9.15.1"
}
